{
	"moduleFileExtensions": [
		"js",
		"json",
		"ts"
	],
	"rootDir": ".",
	"modulePaths": [
		"<rootDir>/../"
	],
	"moduleNameMapper": {
		"^~/(.*)$": "<rootDir>/../../$1"
	},
	"testEnvironment": "node",
	"testRegex": ".e2e-spec.ts$",
	"transform": {
		"^.+\\.(t|j)s$": "ts-jest"
	},
	"globalSetup" : "../prisma/seed.ts"
}